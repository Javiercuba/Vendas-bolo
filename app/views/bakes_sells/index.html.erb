<p id="notice"><%= notice %></p>

<h1>Bakes Sells</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Date sell</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bakes_sells.each do |bakes_sell| %>
      <tr>
        <td><%= bakes_sell.name %></td>
        <td><%= bakes_sell.price %></td>
        <td><%= bakes_sell.date_sell %></td>
        <td><%= bakes_sell.quantity %></td>
        <td><%= link_to 'Show', bakes_sell %></td>
        <td><%= link_to 'Edit', edit_bakes_sell_path(bakes_sell) %></td>
        <td><%= link_to 'Destroy', bakes_sell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bakes Sell', new_bakes_sell_path %>
